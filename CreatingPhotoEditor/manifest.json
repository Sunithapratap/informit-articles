{
	"name" : "Photo Editor",
	"version" : "0.0.6",
	"manifest_version" : 2,
  
	"description" : "Example of a simple Chrome app",
	"icons": {
        "16":"icon16.png",
		"48":"icon48.png",
		"128":"icon128.png"
	},
	"permissions" : [
		"tabs", "notifications"
	],
	"app": {
		"launch" : {
			"local_path" : "index.html"
		}
	},
	"background" : {
		"scripts" : ["js/background.js"]
	}
}
